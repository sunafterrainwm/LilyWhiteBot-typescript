		"module-alias": "^2.2.2",
		"request": "~2.88.2",
		"sharp": "^0.30.3",
		"string-format": "^2.0.0",
		"telegraf": "^4.7.0",
		"typescript": "^4.6.3",
		"winston": "^3.6.0",
		"xml2js": "^0.4.23"
