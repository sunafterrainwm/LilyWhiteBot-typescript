{
	"name": "lilywhitebot",
	"version": "2.0.1-alpha.1",
	"description": "多聊天群互联机器人（目前支持 QQ、Telegram、IRC、Discord）",
	"repository": "git+https://github.com/sunafterrainwm/LilyWhiteBot-typescript.git",
	"license": "AGPL-3.0-or-later",
	"bugs": {
		"url": "https://github.com/sunafterrainwm/LilyWhiteBot-typescript/issues"
	},
	"scripts": {
		"prestart": "grunt built",
		"start": "node src/main.js",
		"ts-start": "ts-node -r tsconfig-paths/register --preferTsExts src/main.ts",
		"test": "npm run lint && npm run build && npm start",
		"lint": "eslint src/**.ts src/**/**.ts",
		"build": "grunt",
		"buildimage": "docker build . -t lilywhitebot",
		"install-start": "pnpm install --frozen-lockfile && npm run ts-start",
		"install-build-start": "pnpm install --frozen-lockfile && npm run build && npm run start",
		"initial": "node scripts/initial-plugins.js"
	},
	"dependencies": {
		"axios": "^1.1.2",
		"chokidar": "^3.5.3",
		"discord.js": "^13.6.0",
		"form-data": "^4.0.0",
		"https-proxy-agent": "^5.0.1",
		"irc-colors": "~1.5.0",
		"irc-upd": "~0.11.0",
		"js-yaml": "^4.1.0",
		"lodash": "^4.17.21",
		"lru-cache": "^7.8.1",
		"module-alias": "^2.2.2",
		"sharp": "^0.30.3",
		"string-format": "^2.0.0",
		"telegraf": "^4.7.0",
		"typescript": "^4.6.3",
		"winston": "^3.7.2"
	},
	"devDependencies": {
		"@types/grunt": "^0.4.27",
		"@types/js-yaml": "^4.0.5",
		"@types/lodash": "^4.14.181",
		"@types/module-alias": "^2.0.1",
		"@types/node": "^17.0.45",
		"@types/sharp": "^0.30.2",
		"@types/string-format": "^2.0.0",
		"@types/xml2js": "^0.4.10",
		"@typescript-eslint/eslint-plugin": "^5.19.0",
		"@typescript-eslint/parser": "^5.19.0",
		"eslint": "^8.13.0",
		"eslint-config-wikimedia": "^0.22.1",
		"eslint-import-resolver-typescript": "^2.7.1",
		"eslint-plugin-import": "^2.26.0",
		"grunt": "^1.5.2",
		"grunt-contrib-clean": "^2.0.0",
		"ts-node": "^10.8.1",
		"tsconfig-paths": "^4.0.0",
		"typegram": "^3.10.0"
	},
	"packageManager": "pnpm@7.13.2",
	"engines": {
		"node": ">=16.0.0"
	}
}
